{
  "name": "vormex-backend",
  "version": "0.1.0",
  "description": "Professional social networking app backend for students (like LinkedIn + Instagram reels + WhatsApp and Instagram messaging features)",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "prisma generate && tsc && cp src/openapi.yaml dist/",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "neon:keep-alive": "node scripts/neon-keep-alive.js",
    "docs:generate": "swagger-cli bundle -o src/openapi.json src/openapi.yaml",
    "docs:validate": "swagger-cli validate src/openapi.yaml",
    "docs:serve": "npm run docs:generate && npm start"
  },
  "keywords": [
    "social-networking",
    "backend",
    "express",
    "typescript",
    "prisma"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/bull": "^3.15.9",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "firebase-admin": "^13.6.1",
    "google-auth-library": "^10.5.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "qrcode": "^1.5.4",
    "resend": "^6.5.2",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.3",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.7.9",
    "@types/qrcode": "^1.5.6",
    "@types/sharp": "^0.32.0",
    "@types/speakeasy": "^2.0.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "prisma": "^5.19.1",
    "swagger-cli": "^4.0.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
